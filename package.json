{
  "name": "nodejs-tinyurl",
  "version": "1.0.0",
  "description": "shorten your URLs online",
  "main": "index.js",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "nodemon -r dotenv/config src/index.js dotenv_config_path=./dev.env",
